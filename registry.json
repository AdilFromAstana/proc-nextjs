{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "Design Registry",
  "homepage": "https://design-starter.vercel.app",
  "items": [
    {
      "name": "starter",
      "type": "registry:theme",
      "title": "Starter Prototype",
      "description": "Blank application with your brand themed components",
      "files": [
        {
          "path": "src/styles/globals.css",
          "type": "registry:file",
          "target": "styles/globals.css"
        },
        {
          "path": "src/styles/tailwind.config.ts",
          "type": "registry:file",
          "target": "tailwind.config.ts"
        },
        {
          "path": "src/app/starter/layout.tsx",
          "type": "registry:file",
          "target": "app/layout.tsx"
        },
        {
          "path": "src/app/starter/page.tsx",
          "type": "registry:file",
          "target": "app/page.tsx"
        },
        {
          "path": "src/components/brand-header.tsx",
          "type": "registry:file",
          "target": "components/brand-header.tsx"
        },
        {
          "path": "src/components/logo.tsx",
          "type": "registry:file",
          "target": "components/logo.tsx"
        },
        {
          "path": "src/components/brand-sidebar.tsx",
          "type": "registry:file",
          "target": "components/brand-sidebar.tsx"
        }
      ]
    },
    {
      "name": "brand-header",
      "type": "registry:component",
      "title": "Brand Header",
      "description": "A styled, simple, reusable header",
      "registryDependencies": ["button", "input", "avatar", "sidebar"],
      "files": [
        {
          "path": "src/styles/globals.css",
          "type": "registry:file",
          "target": "styles/globals.css"
        },
        {
          "path": "src/styles/tailwind.config.ts",
          "type": "registry:file",
          "target": "tailwind.config.ts"
        },
        {
          "path": "src/components/brand-header.tsx",
          "type": "registry:file",
          "target": "components/brand-header.tsx"
        },
        {
          "path": "src/components/logo.tsx",
          "type": "registry:file",
          "target": "components/logo.tsx"
        }
      ]
    },
    {
      "name": "brand-sidebar",
      "type": "registry:component",
      "title": "Brand Sidebar",
      "description": "A styled, simple, reusable sidebar",
      "registryDependencies": ["badge", "button", "sidebar"],
      "files": [
        {
          "path": "src/styles/globals.css",
          "type": "registry:file",
          "target": "styles/globals.css"
        },
        {
          "path": "src/styles/tailwind.config.ts",
          "type": "registry:file",
          "target": "tailwind.config.ts"
        },
        {
          "path": "src/components/brand-sidebar.tsx",
          "type": "registry:file",
          "target": "components/brand-sidebar.tsx"
        }
      ]
    },
    {
      "name": "button",
      "type": "registry:component",
      "title": "Button",
      "description": "A styled, simple, reusable button",
      "registryDependencies": ["button"],
      "files": [
        {
          "path": "src/styles/globals.css",
          "type": "registry:file",
          "target": "styles/globals.css"
        },
        {
          "path": "src/styles/tailwind.config.ts",
          "type": "registry:file",
          "target": "tailwind.config.ts"
        }
      ]
    }
  ]
}
